{
  "name": "llm-warehouse",
  "version": "0.3.0",
  "description": "Auto-capture OpenAI and Anthropic LLM calls for warehousing and observability",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev": "tsc --watch",
    "test": "vitest run --reporter=dot",
    "test:watch": "vitest",
    "test:node": "vitest run --project node",
    "test:browser": "vitest run --project browser",
    "test:edge": "vitest run --project edge",
    "test:types": "tsd",
    "test:no-api": "node -e \"console.log('Skipping tests')\""
  },
  "keywords": [
    "llm",
    "openai",
    "anthropic",
    "logging",
    "observability",
    "monitoring",
    "warehouse"
  ],
  "author": "Sinan Ozdemir <sinan.u.ozdemir@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "openai": "^4.0.0",
    "ai": "^5.0.0",
    "@ai-sdk/openai": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "dotenv": "^16.0.0",
    "miniflare": "^3.20240904.0",
    "ts-node": "^10.0.0",
    "tsd": "^0.30.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0"
  },
  "sideEffects": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Agenti-Code/llm-warehousing-npm.git"
  },
  "bugs": {
    "url": "https://github.com/Agenti-Code/llm-warehousing-npm/issues"
  },
  "homepage": "https://github.com/Agenti-Code/llm-warehousing-npm#readme"
}
